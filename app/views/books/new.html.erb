<h1>Sell a textbook!</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@book) do |f| %>
      <%= render 'shared/error_messages', object: f.object %>
      <%= f.label :name %>
      <%= f.text_field :name %>

      <%= f.label :college %>
      <%= f.email_field :college %>


      <%= f.submit "Submit book info", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>